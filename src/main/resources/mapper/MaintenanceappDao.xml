    </set>
    where maintenanceappid = #{maintenanceappid,jdbcType=INTEGER}
  </update>
  <!--  改收款状态-->
  <update id="updatemaintenanceappmony" parameterType="com.trkj.asms.entity.Maintenanceapp">
