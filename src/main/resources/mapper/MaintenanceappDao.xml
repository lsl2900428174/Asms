    #{empid,jdbcType=INTEGER},#{xiangmuid,jdbcType=INTEGER},#{cailiaoid,jdbcType=INTEGER},#{mendianid,jdbcType=INTEGER},
     #{appno,jdbcType=VARCHAR}, #{yujiallmony,jdbcType=DECIMAL},
      #{yujisetcar,jdbcType=TIMESTAMP}, #{yujisetcartime,jdbcType=TIMESTAMP},#{kehudepict,jdbcType=VARCHAR},
      #{shiyucause,jdbcType=VARCHAR},#{beizhu,jdbcType=VARCHAR},#{carid,jdbcType=INTEGER})
  </insert>
<!--  改状态-->
